# my.kv

        



<user_login>:
    MDBoxLayout:
        
        

        MDScreen:
            MDBoxLayout:
                    
                orientation:'vertical'
                size_hint:None,None
                width:100
                height:100
                spacing :25
                pos_hint:{'center_x':0.5,'center_y':0.5}
                padding:[30,30,30,30]
                
                MDBoxLayout:
                    size_hint:None,None
                    pos_hint:{'center_x':0.5,'center_y':0.5}
                    orientation:'vertical'
                    size: 400,200
                    Image:
                        source:r"logo.png"

               
                   
        
            MDNavigationLayout:

            MDScreenManager:

                MDScreen:
                    id:label_scr

                    MDTopAppBar:
                        title: "ITX-SOLUTION"
                        elevation: 4
                        pos_hint: {"top": 1}
                        md_bg_color: "#050b78"
                        specific_text_color: "#ffffff"
                        left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]

            MDNavigationDrawer:
                id: nav_drawer
                radius: (0, 16, 16, 0)
                width:'200dp'
                
                MDNavigationDrawerMenu:
                    spacing:10

                    MDNavigationDrawerLabel:
                        text: "LogOut"

                    MDNavigationDrawerDivider:

                    MDIconButton:
                        icon: "logout"
                        width:'50sp'
                        on_release:root.logOut()

                    

                    MDNavigationDrawerLabel:
                        text: "Options"
                        

                    MDNavigationDrawerDivider:

                    
                        
                        
                    MDFlatButton:
                        
                        text: "Status"
                        md_bg_color : (0, 0, 1, 1)
                        size_hint:0.5,None
                        theme_text_color : "Custom"
                        text_color:1,1,1,1
                        on_release:root.status()
                
                

                    MDFlatButton:
                        
                        text: "Hours"
                        md_bg_color:0,0,1,1
                        size_hint:0.5,None
                        theme_text_color : "Custom"
                        text_color:1,1,1,1
                        on_release:root.check_hour()
                    
                
                    MDFlatButton:
                        
                        text: "Main"
                        md_bg_color:0,0,1,1
                        size_hint:0.5,None
                        theme_text_color : "Custom"
                        text_color:1,1,1,1
                        on_release:root.after_login()
                



